  "bugs": {
    "url": "https://github.com/oto-hu/my-new-project/issues"
  },
  "homepage": "https://github.com/oto-hu/my-new-project#readme",
  "devDependencies": {
    "eslint": "^9.27.0",
    "eslint-plugin-html": "^8.1.3",
    "jest": "^29.7.0",
    "live-server": "^1.2.2",
    "prettier": "^3.5.3"
  }
}
