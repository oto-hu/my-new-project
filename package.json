{
  "name": "my-new-project",
  "version": "1.0.0",
  "description": "Static HTML website project",
  "main": "index.html",
  "scripts": {
    "test": "jest --passWithNoTests",
    "lint": "eslint . --ext .js,.html --ignore-pattern node_modules",
    "format": "prettier --write *.html css/*.css js/*.js",
    "serve": "live-server --port=3000",
    "build": "npm run lint && echo 'Static site ready for deployment'"
